@model List<Course>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Course list Page</h1>

    @foreach(var i in Model)
    {
        <div id="kurs_@i.ID">
            <h2>@i.Title</h2>
            <p>@i.Description</p>
            <img src="~/img/@i.Image" width="200" alt="">
        </div>
    }
</body>
</html>